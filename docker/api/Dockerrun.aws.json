{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "essential": true,
            "image": "delegateit/gatapi:25f96ff",
            "memory": 512,
            "mountPoints": [
                {
                    "containerPath": "/var/gator/api",
                    "readOnly": true,
                    "sourceVolume": "apisource"
                }
            ],
            "name": "api",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 80
                }
            ]
        }
    ],
    "volumes": [
        {
            "host": {
                "sourcePath": "/var/app/current/apisource"
            },
            "name": "apisource"
        }
    ]
}